{
  "name": "todo-app",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --coverage",
    "build-web": "expo export -p web",
    "cypress": "cypress open"
  },
  "dependencies": {
    "@expo/metro-runtime": "~4.0.0",
    "@testing-library/react-native": "^13.0.0",
    "expo": "~52.0.25",
    "expo-checkbox": "~4.0.1",
    "expo-status-bar": "~2.0.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.6",
    "react-native-check-box": "^2.1.7",
    "react-native-web": "~0.19.13"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/jest": "^29.5.14",
    "@types/react": "~18.3.12",
    "cypress": "^14.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-expo": "^52.0.3",
    "typescript": "^5.3.3"
  },
  "private": false,
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@sentry/react-native|native-base|react-native-svg)",
      "\\.css$"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  },
  "description": "to do",
  "repository": "https://github.com/Gass-Dev/todo-app.git",
  "author": "Gass <madebygass@gmail.com>"
}
